<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@android:drawable/ic_delete" />

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/achbutt1"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_marginTop="8dp"
        android:layout_below="@+id/imageButton1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@android:drawable/ic_delete" />

    <Button
        android:id="@+id/button2"
        android:layout_width="1000dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button1"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_toRightOf="@+id/imageButton2"
        android:text="@string/achbutt2" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_marginTop="8dp"
        android:layout_below="@+id/imageButton2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@android:drawable/ic_delete"/>

    <Button
        android:id="@+id/button3"
        android:layout_width="1000dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button2"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="1dp"
        android:layout_toRightOf="@+id/imageButton3"
        android:text="@string/achbutt3"
        />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_marginTop="8dp"
        android:layout_below="@+id/imageButton3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@android:drawable/ic_delete"/>

    <Button
        android:id="@+id/button4"
        android:layout_width="1000dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button3"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="-1dp"
        android:layout_toRightOf="@+id/imageButton4"
        android:text="@string/achbutt4"  />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_marginTop="8dp"
        android:layout_below="@+id/imageButton4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@android:drawable/ic_delete"/>

    <Button
        android:id="@+id/button5"
        android:layout_width="1000dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button4"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="-1dp"
        android:layout_toRightOf="@+id/imageButton5"
        android:text="@string/achbutt5"  />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_marginTop="8dp"
        android:layout_below="@+id/imageButton5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@android:drawable/ic_delete"/>

    <Button
        android:id="@+id/button6"
        android:layout_width="1000dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button5"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="-1dp"
        android:layout_toRightOf="@+id/imageButton6"
        android:text="@string/achbutt6" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_marginTop="8dp"
        android:layout_below="@+id/imageButton6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@android:drawable/ic_delete"/>

    <Button
        android:id="@+id/button7"
        android:layout_width="1000dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button6"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="-1dp"
        android:layout_toRightOf="@+id/imageButton7"
        android:text="@string/achbutt7"  />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_marginTop="8dp"
        android:layout_below="@+id/imageButton7"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@android:drawable/ic_delete"/>

    <Button
        android:id="@+id/button8"
        android:layout_width="1000dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button7"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="0dp"
        android:layout_toRightOf="@+id/imageButton8"
        android:text="@string/achbutt8"  />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_marginTop="8dp"
        android:layout_below="@+id/imageButton8"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@android:drawable/ic_delete"/>

    <Button
        android:id="@+id/button9"
        android:layout_width="1000dp"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/imageButton9"
        android:text="@string/achbutt9"
        android:layout_alignParentBottom="false"
        android:layout_alignTop="@+id/imageButton9" />
</RelativeLayout>
